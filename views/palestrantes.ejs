    <h2> Sábado 14/05/11</h2>

   <table id="programacao">

      <tr>
        <td class="subHorario">Horário</td>
        <td class="subTitulo">Palestras - Palestrantes</td>
      </tr>
      <tpl for=".">
        <tr>
          <td class="horario">{horario}</td>
          <td> {[values.palestra.tema]}
            <tpl if="palestra.palestrantes">
              <tpl for="palestra.palestrantes">
                <br/>
                <div>
                  <img src="{photo}"  style="float:left;"/>
                  <div>{name}</div>
                  <div><a href="http://www.twitter.com/#!{twitter}">{twitter}</a></div>
                  <div><a href="{site}">{site}</a></div>
                </div>
              </tpl>
            </tpl>
          <br/>
          
          
            <tpl if="palestra.lightning">
              <tpl for="palestra.lightning">
                <table id="programacao">
                  <tr>
                    <td>{horario}</td>
                    <td>{[values.palestra.tema]}</td>
                  </tr>
                </table>
              </tpl>
            </tpl>
          </td>
        </tr>
